{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/theme.json",
  "name": "RTScript Default Theme",
  "settings": [
    {
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#1E1E1E"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#C586C0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.other",
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "scope": "string.quoted.double",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6A9955",
        "fontStyle": "italic"
      }
    }
  ]
}